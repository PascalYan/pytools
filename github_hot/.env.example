# GitHub配置
GITHUB_TOKEN=your_github_token
GITHUB_LANGUAGE=Python  # 空字符串表示所有语言
GITHUB_SINCE=weekly    # daily/weekly/monthly
GITHUB_LIMIT=10        # 获取的仓库数量限制

# 本地文件发布器配置
ENABLE_LOCAL_FILE=true                           # 是否启用本地文件发布器
LOCAL_OUTPUT_DIR=/path/to/your/articles          # 文章输出目录
LOCAL_FILENAME_TEMPLATE={date}_{title}.md        # 文件名模板，支持变量：{date}, {time}, {title}

# 微信公众号配置
ENABLE_WECHAT_MP=false                          # 是否启用微信公众号发布器
WECHAT_APP_ID=your_app_id                       # 微信公众号AppID
WECHAT_APP_SECRET=your_app_secret               # 微信公众号AppSecret
WECHAT_DIRECT_PUBLISH=false                    # 是否直接发布文章（默认保存为草稿）

# 企业微信群机器人配置
ENABLE_WECHAT_WEBHOOK=false                     # 是否启用企业微信群机器人
WECHAT_ARTICLE_WEBHOOK=your_webhook_url         # 文章推送Webhook地址
WECHAT_ALERT_WEBHOOK=your_alert_webhook_url     # 告警推送Webhook地址
WECHAT_ARTICLE_URL_SOURCE=csdn                  # 文章URL来源(csdn/juejin/devto/confluence/yuque/notion)

# Confluence配置
ENABLE_CONFLUENCE=false                         # 是否启用Confluence发布器
CONFLUENCE_BASE_URL=your_confluence_url         # Confluence基础URL
CONFLUENCE_USERNAME=your_username               # Confluence用户名
CONFLUENCE_API_TOKEN=your_api_token            # Confluence API Token
CONFLUENCE_SPACE_KEY=your_space_key            # Confluence空间Key
CONFLUENCE_PARENT_PAGE_ID=your_parent_page_id  # 父页面ID

# 语雀配置
ENABLE_YUQUE=false                             # 是否启用语雀发布器
YUQUE_TOKEN=your_token                         # 语雀Token
YUQUE_NAMESPACE=your_namespace                 # 语雀知识库命名空间
YUQUE_SLUG=your_slug                          # 文档路径

# Notion配置
ENABLE_NOTION=false                            # 是否启用Notion发布器
NOTION_TOKEN=your_token                        # Notion Token
NOTION_DATABASE_ID=your_database_id           # Notion数据库ID

# CSDN配置
ENABLE_CSDN=false                             # 是否启用CSDN发布器
CSDN_USERNAME=your_username                   # CSDN用户名
CSDN_PASSWORD=your_password                   # CSDN密码
CSDN_COOKIE=your_cookie                      # CSDN Cookie

# 掘金配置
ENABLE_JUEJIN=false                           # 是否启用掘金发布器
JUEJIN_USER_ID=your_user_id                  # 掘金用户ID
JUEJIN_COOKIE=your_cookie                    # 掘金Cookie

# Dev.to配置
ENABLE_DEVTO=false                            # 是否启用Dev.to发布器
DEVTO_API_KEY=your_api_key                   # Dev.to API Key 